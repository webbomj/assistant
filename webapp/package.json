{
  "name": "@assistant/webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx .",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelintcache \"src/**/*.scss\"",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheckcache",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.json && tsc --noEmit --project ./tsconfig.node.json",
    "prettify": "prettier --loglevel warn --cache --write '{src/**/*,*}.{ts,tsx,js,json,yml,scss}' '!**/{node_modules,dist}/**/*'"
  },
  "dependencies": {
    "@assistant/backend": "workspace:*",
    "classnames": "^2.5.1",
    "date-fns": "^3.3.1",
    "formik": "^2.4.5",
    "formik-validator-zod": "^2.0.0",
    "include-media": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "reset-css": "^5.0.2",
    "sass": "^1.70.0",
    "superjson": "^1.13.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tanstack/react-query": "^4.35.3",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "vite": "^5.0.8"
  }
}
